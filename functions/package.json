{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "serve": "npm run environmentDev && cd .. && firebase emulators:start --only hosting,functions,firestore,auth,ui --import=./emulator-data --export-on-exit=./emulator-data",
    "serveProd": "npm run environmentProd && cd .. && firebase emulators:start --only functions,ui",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "environmentProd": "firebase functions:config:set environment.mode=production && firebase functions:config:get > config.json && firebase functions:config:get",
    "environmentDev": "firebase functions:config:set environment.mode=development && firebase functions:config:get > config.json && firebase functions:config:get"
  },
  "engines": {
    "node": "14"
  },
  "main": "index.js",
  "dependencies": {
    "@google-cloud/storage": "^5.14.3",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/sitemap": "^2.4.0",
    "algoliasearch": "^4.11.0",
    "axios": "^0.21.1",
    "busboy": "^0.3.1",
    "busboy-firebase": "^1.0.8",
    "codemirror": "^5.63.3",
    "cookieparser": "^0.1.0",
    "core-js": "^3.9.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-multipart-file-parser": "^0.1.2",
    "express-validator": "^6.12.2",
    "firebase": "^8.4.1",
    "firebase-admin": "^9.2.0",
    "firebase-functions": "^3.11.0",
    "instantsearch.css": "^7.4.5",
    "instantsearch.js": "^4.33.1",
    "javascript-time-ago": "^2.3.10",
    "js-cookie": "^3.0.0",
    "jwt-decode": "^3.1.2",
    "nuxt": "^2.15.3",
    "showdown": "^1.9.1",
    "tiptap-vuetify": "^2.8.0",
    "turndown": "^7.1.1",
    "vue-instantsearch": "^4.1.1",
    "vue-observe-visibility": "^1.0.0",
    "vuelidate": "^0.7.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@nuxtjs/vuetify": "^1.12.1",
    "firebase-functions-test": "^0.2.0"
  },
  "private": true
}
