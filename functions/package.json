{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "serve": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "fe": "cd .. && firebase emulators:start --import=./emulator-data --export-on-exit=./emulator-data"
  },
  "engines": {
    "node": "14"
  },
  "main": "index.js",
  "dependencies": {
    "@nuxt/content": "^1.14.0",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/firebase": "^7.6.1",
    "@nuxtjs/pwa": "^3.3.5",
    "axios": "^0.21.1",
    "cookieparser": "^0.1.0",
    "core-js": "^3.9.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "firebase": "^8.4.1",
    "firebase-admin": "^9.2.0",
    "firebase-functions": "^3.11.0",
    "js-cookie": "^3.0.0",
    "jwt-decode": "^3.1.2",
    "nuxt": "^2.15.3",
    "video.js": "^7.14.3",
    "vuelidate": "^0.7.6",
    "vuetify": "^2.5.8",
    "vuetify-loader": "^1.7.3"
  },
  "devDependencies": {
    "firebase-functions-test": "^0.2.0",
    "@nuxtjs/vuetify": "^1.12.1"
  },
