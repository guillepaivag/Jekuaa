{
  "functions": {
    "predeploy": [],
    "source": "functions"
  },
  "hosting": {
    "site": "jekuaa-py",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "defaultNuxtSSR"
      }
    ]
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "ui": {
      "enabled": true
    }
  }
}
