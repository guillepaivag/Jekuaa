{
  "functions": {
    "predeploy": [],
    "source": "functions"
  },
  "hosting": {
    "site": "jekuaa-py",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "**/emulator-data/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "defaultJekuaaNuxtSSR"
      }
    ]
  },
  "emulators": {
    "auth": {
      "port": 9099
    },
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "hosting": {
      "port": 5000
    },
    "ui": {
      "enabled": true
    }
  },
  "firestore": {
    "rules": "firestore.rules"
  },
  "storage": {
    "rules": "storage.rules"
  }
}